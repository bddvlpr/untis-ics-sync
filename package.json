{
  "name": "units-ics-sync",
  "version": "0.2.8",
  "description": "Serves a calendar API (ICS) for events provided from Untis.",
  "main": "src/index.ts",
  "repository": "https://github.com/bddvlpr/untis-ics-sync.git",
  "author": "Luna Simons <luna@bddvlpr.com>",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "test": "jest --forceExit",
    "commit": "cz",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. . && eslint . --fix"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^2.29.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "ics": "^2.37.0",
    "redis": "^4.2.0",
    "webuntis": "^1.22.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "commitizen": "^4.2.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "git-cz": "^4.9.0",
    "jest": "^29.0.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
